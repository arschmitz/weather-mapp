<div class="display">
	<div class="row display--box">
		<div class="col-xs-12"><h1 class="display--title">{{model.meta.name}} Satellite Imagery</h1></div>
	</div>
	<div class="row around-xs">
		{{#each-in model.images as |key img|}}
		<div class="col-xs-12 col-md-6 col-lg-3 display--box satellite--wrap">
			<div class="satellite--box">
				<div class="satellite--title">{{img.name}} Image</div>
				<img src="{{img.url}}" alt="satellite image">
			</div>
		</div>
		{{/each-in}}
	</div>
</div>